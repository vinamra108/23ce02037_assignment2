#include<stdio.h>
int main() {
    int x,y,z;
    printf("Enter three integers: ");
    scanf("%d %d %d",&x,&y,&z);
    if(x>y) {
        if(x>z) {
            printf("%d is the largest integer",x);
        } else {
            printf("%d is the largest integer",z);
        }
    }
    else if(y>x) {
        if(y>z) {
            printf("%d is the largest integer",y);
        } else {
            printf("%d is the largest integer",z);
        }
    }
    return 0;
}